# Morpheus IDE Environment Configuration

# OpenAI API Configuration
# Get your API key from: https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Custom OpenAI base URL (for proxies or alternative endpoints)
# REACT_APP_OPENAI_BASE_URL=https://api.openai.com/v1

# Qdrant Vector Database Configuration
# Default local Qdrant instance
REACT_APP_QDRANT_URL=http://localhost:6333

# Optional: Qdrant API key (for cloud instances)
# REACT_APP_QDRANT_API_KEY=your_qdrant_api_key_here

# Optional: Custom collection name for vector storage
# REACT_APP_QDRANT_COLLECTION=morpheus-code

# Development Mode Settings
# Set to 'true' to enable debug logging
# REACT_APP_DEBUG=false

# Set to 'true' to use mock AI services (no API keys required)
# REACT_APP_MOCK_AI=false

# Performance Settings
# Maximum file size to process (in bytes)
# REACT_APP_MAX_FILE_SIZE=1048576

# Chunk size for code parsing
# REACT_APP_CHUNK_SIZE=1000

# Chunk overlap for better context
# REACT_APP_CHUNK_OVERLAP=200

# AI Model Configuration
# Default embedding model
# REACT_APP_EMBEDDING_MODEL=text-embedding-ada-002

# Default chat model
# REACT_APP_CHAT_MODEL=gpt-4

# Maximum tokens for AI responses
# REACT_APP_MAX_TOKENS=4000

# UI Configuration
# Enable/disable AI features
# REACT_APP_ENABLE_AI_CHAT=true
# REACT_APP_ENABLE_AI_COMPLETION=true
# REACT_APP_ENABLE_SEMANTIC_SEARCH=true
# REACT_APP_ENABLE_CODE_INSIGHTS=true

# Suggestion delay in milliseconds
# REACT_APP_SUGGESTION_DELAY=1000

# Maximum number of suggestions to show
# REACT_APP_MAX_SUGGESTIONS=5

# Security Settings
# Enable/disable real-time indexing
# REACT_APP_ENABLE_REAL_TIME_INDEXING=true

# File patterns to exclude from indexing (comma-separated)
# REACT_APP_EXCLUDE_PATTERNS=node_modules/**,.git/**,dist/**,build/**

# File patterns to include in indexing (comma-separated)
# REACT_APP_INCLUDE_PATTERNS=**/*.ts,**/*.tsx,**/*.js,**/*.jsx,**/*.py,**/*.java
